{
  "extends": "airbnb-base",
  "rules": {
    "indent": 0,
    "no-tabs": 0,
    "object-shorthand": 0,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "no-unused-vars": 0,
    "no-var": 0,
    "no-undef": 0,
    "no-console": 0,
    "import/no-unresolved": 0,
    "prefer-destructuring": 0,
    "eqeqeq": 0,
    "no-useless-escape": 0,
    "no-restricted-syntax": 0,
    "prefer-template": 0,
    "no-lonely-if": 0,
    "no-use-before-define": 0,
    "max-len": 0,
    "guard-for-in": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": [error, { devDependencies: true }]
  }
}